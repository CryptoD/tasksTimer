#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:42
msgid "Show a notification when a timer ends"
msgstr "Afficher une notification à la fin de la minuterie"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:47
msgid "Inhibit screen lock"
msgstr "Désactiver le verrouillage de l'écran"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:48
msgid ""
"0 - off 1 - Inhibit logging out 2 - Inhibit user switching 4 - Inhibit "
"suspending the session or computer 8 - Inhibit the session being marked as "
"idle"
msgstr ""
"0 - off 1 - Inhibit logging out 2 - Inhibit user switching 4 - Inhibit "
"suspending the session or computer 8 - Inhibit the session being marked as "
"idle"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:58
msgid "Maximum amount of time to inhibit screen lock, in seconds"
msgstr "Temps maximum pour empêcher le verrouillage de l'écran, en secondes"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:59
msgid "0 is no maximum, positive integer specifies the number of seconds"
msgstr ""
"0 n'est pas un maximum, une valeur positive spécifie le nombre de secondes"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:64
msgid "Make the notification wait for input"
msgstr "Faire attendre la notification pour l'entrée de l'utilisateur"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:69
msgid "Long timeout for sticky notifications, default is 30 seconds"
msgstr ""
"Long délai d'attente pour les notifications persistantes, la valeur par "
"défaut est de 30 secondes"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:73
msgid "Show the time when the timer will expire rather than the countdown"
msgstr ""
"Afficher le temps lorsque la minuterie expirera plutôt que le compte à "
"rebours"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:77
msgid "Show time remaining in panel menu"
msgstr "Afficher le temps restant dans le panneau"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:81
msgid "Show timer progress in panel menu"
msgstr "Afficher la progression de la minuterie dans le panneau"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:85
msgid "Show timer name in panel menu"
msgstr "Afficher le nom de la minuterie dans le panneau"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:89
msgid "Play sound notifications"
msgstr "Jouer les sons de notification"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:94
msgid "Number of times to ring alarm sound, 0 is the duration of notification"
msgstr "Nombre de sonneries d'alarme, 0 est la durée de la notification"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:98
msgid "Path to sound file"
msgstr "Chemin vers le fichier son"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:103
msgid "Default length of timer in seconds"
msgstr "Durée par défaut de la minuterie en secondes"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:107
msgid "Array of Timer descriptions"
msgstr "Tableau de descriptions de minuterie"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:111
msgid "Array of Quick Timer descriptions"
msgstr "Table of descriptions of convenient timers"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:115
msgid "timers configuration stored as json"
msgstr "configuration des minuteries stockée en tant que json"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:116
msgid "Experimental feature for storing timer data"
msgstr "Fonction expérimentale de stockage des données de minuterie"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:120
msgid "Sort list of timers by duration"
msgstr "Trier la liste des minuteries par durée"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:124
msgid "Sort from longest to shortest"
msgstr "Trier du plus long au plus court"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:128
msgid "Save quick timers between sessions if enabled"
msgstr "Enregistrer des minuteries pratiques entre les sessions si activé"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:132
msgid "Prevent creation of duplicate timers of the same type"
msgstr "Eviter la création de minuteries en double du même type"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:136
msgid "State of running timers as json string"
msgstr "État des minuteries en cours d'exécution comme json"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:141
msgid "Low volume threshold in percent"
msgstr "Limite de volume faible en pourcentage"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:146
msgid ""
"Heuristic counter to prefer displaying the quick or preset menus expanded"
msgstr ""
"Compteur heuristique pour préférer afficher les menus rapides ou préréglées"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:150
msgid "Warn if volume levels are below threshold"
msgstr "Avertir si les niveaux de volume sont inférieurs au seuil"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:154
msgid "Turn on debugging"
msgstr "Activer le débogage"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:158
msgid "Enable keyboard shortcuts"
msgstr "Activer les raccourcis clavier"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:162
msgid "Keyboard shortcut for toggling show-end-time setting"
msgstr "Accélérateur de clavier pour basculer le paramètre show-end-time"

#: ../schemas/org.gnome.shell.extensions.tasktimer.gschema.xml:166
msgid "Keyboard shortcut to stop the next timer to expire"
msgstr "Accélérateur de clavier pour arrêter la prochaine minuterie à expirer"

